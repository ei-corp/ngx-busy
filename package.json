{
  "name": "angular-opensource",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "transpile": "ngc",
    "start": "ng build ng-busy && ng serve",
    "prebuild": "rimraf ./dist/ng-busy",
    "build": "ng build ng-busy --configuration production",
    "postbuild": "lessc ./projects/ng-busy/src/lib/style/busy.less ./dist/ng-busy/src/style/busy.css",
    "publish": "npm run build && npm publish ./dist/ng-busy",
    "minify": "uglifyjs dist/ng-busy/ng-busy.umd.js --compress --mangle --comments --output dist/ng-busy/ng-busy.umd.min.js",
    "build-new": "npm run package && npm run minify",
    "pack": "rimraf -g ./*.tgz && npm pack ./dist"
  },
  "dependencies": {
    "@angular/animations": "^20.1.4",
    "@angular/cdk": "^20.1.4",
    "@angular/common": "^20.1.4",
    "@angular/compiler": "^20.1.4",
    "@angular/core": "^20.1.4",
    "@angular/forms": "^20.1.4",
    "@angular/material": "^20.1.4",
    "@angular/platform-browser": "^20.1.4",
    "@angular/platform-browser-dynamic": "^20.1.4",
    "@angular/router": "^20.1.4",
    "code-prettify": "^0.1.0",
    "codelyzer": "^6.0.2",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.1.4",
    "@angular-devkit/core": "20.1.4",
    "@angular/cli": "20.1.4",
    "@angular/compiler-cli": "^20.1.4",
    "@angular/language-service": "^20.1.4",
    "@types/jasmine": "~5.1.7",
    "@types/jasminewd2": "~2.0.13",
    "@types/node": "^20.10.0",
    "jasmine-core": "~5.6.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "less": "^4.2.2",
    "lessc": "^1.0.2",
    "ng-packagr": "^20.1.0",
    "protractor": "~7.0.0",
    "rimraf": "^6.0.1",
    "ts-node": "~10.9.2",
    "tslint": "~6.1.3",
    "typescript": "^5.8.2",
    "uglifyjs": "2.4.11"
  }
}
